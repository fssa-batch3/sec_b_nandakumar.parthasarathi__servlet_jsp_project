<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>findAllUsersServlet</servlet-name>
  	<display-name>findAllUsersServlet</display-name>
  	<description></description>
  	<servlet-class>in.fssa.mynotesweb.servlets.findAllUsersServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>getAllTasks</servlet-name>
  	<display-name>getAllTasks</display-name>
  	<description></description>
  	<servlet-class>in.fssa.mynotesweb.servlets.getAllTasks</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>findAllUsersServlet</servlet-name>
  	<url-pattern>/findAllUsersServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>getAllTasks</servlet-name>
  	<url-pattern>/getAllTasks</url-pattern>
  </servlet-mapping>
</web-app>
